{
    "_id": "_design/cell_views",
    "views": {
      "count_by_agent": {
        "map": "function (doc) { if (doc.type === 'cell' && doc.agents.length > 0) emit(doc.agents, doc); }"
      },
      "by_undiscovered_cells": {
        "map": "function (doc) { if (doc.type === 'cell' && doc.valeur === 0) { emit(null, doc); } }"
      },
      "by_cells": {
        "map": "function (doc) { if (doc.type === 'cell' ) { emit([doc.x, doc.y], doc); } }"
      },
      "undiscovered_neighbors": {
        "map": "function (doc) { if (doc.type === 'cell' && doc.valeur === 0) { emit([doc.x, doc.y], doc); } }"
      }
    }
  }